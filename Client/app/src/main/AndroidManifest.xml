<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.client" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.client.account.FirstActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.client.account.LoginActivity"
            android:label="Login" >
        </activity>
        <activity
            android:name="com.client.account.RegisterActivity"
            android:label="Register" >
        </activity>
        <activity
            android:name="com.client.account.RetrivePass"
            android:label="Retrive Pass" >
        </activity>
        <activity
            android:name="com.client.activity.MainActivity"
            android:label="Budget App">
        </activity>
        <activity android:name=".account.AccountActivity"></activity>

    </application>

</manifest>
